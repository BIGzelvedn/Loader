-- Prevent double load
if _G.GAME_LOADER_LOADED then return end
_G.GAME_LOADER_LOADED = true

-- Game fully loaded check
if not game:IsLoaded() then
    game.Loaded:Wait()
end

local PlaceId = game.PlaceId

local GameScripts = {
    [198116126] = "loadstring(game:HttpGet("https://raw.githubusercontent.com/BIGzelvedn/Tradelandsv2/refs/heads/main/Tradelands1",true))()",
    [691884101] = "loadstring(game:HttpGet("https://raw.githubusercontent.com/BIGzelvedn/DynamicShip/refs/heads/main/DSS3",true))()",
    [139571938533204] = "loadstring(game:HttpGet("https://raw.githubusercontent.com/BIGzelvedn/BSS/refs/heads/main/BalticSea",true))()",
}

-- load script
local scriptUrl = GameScripts[PlaceId]

if scriptUrl then
    loadstring(game:HttpGet(scriptUrl))()
else
    warn("Game Not Supported | PlaceId:", PlaceId)
end
